<!DOCTYPE html>
<html lang="en">
<head>
    
        <meta charset="UTF-8">
        <meta http-equiv="X-UA-Compatible" content="IE=edge">
        <meta name="viewport" content="width=device-width, initial-scale=1.0">
        <link rel='stylesheet' href='https://cdn-uicons.flaticon.com/uicons-regular-rounded/css/uicons-regular-rounded.css'>
        <link rel="stylesheet" href="page2.css">
    </head>
    <body>
        <div class="top">
            <div class="top1">
                <img src="clogo.png">
            </div>
            <div class="top2">
                <p class="top21">You choose, We do it</p>
                <p class="top22">St. Joseph's College of Engineering</p>
                <p class="top23">Department of computer Science & Engineering</p>
            </div>
            <div class="top3">
                <img src="clogo1.PNG" class="top31">
            </div>
        </div>
        <div class="outer">
            <div class="nav" style="height: 430px;">
                <div class="head_section">
                  <img src="user.png" alt="">
                  <div class="name">
                    <h3><%=username%></h3>
                  </div>
                </div>
                <div class="nav_links">
                  <ul>
                    <li><a href="details"><i class="fi-rr-browser"></i><span>Staff Details</span></a></li>
                    <li><a href="addteacher"><i class="fi-rr-document-signed"></i></i><span>Add Teacher</span></a></li>
                    <li><a href="addsubject"><i class="fi-rr-document-signed"></i></i><span>Add Subject</span></a></li>
                    <li><a href="changepass"><i class="fi-rr-lock"></i></i><span>Change Password</span></a></li>
                    <li><a href="logout"><i class="fi-rr-power"></i><span>Log Out</span></a></li>
                  </ul>
                </div>
              </div>
              <div class="upload">
                  <h3 style="margin-left: 18%;"> Regulation & Subjects</h3>
                  <form action="/addsubject1" method="POST" enctype="multipart/form-data">
                    <div class="uploadinput">
                      <label for="st">Add Subject:</label>
                      <input type="checkbox" style="width:13px;" id="adsub" autocomplete="off" name="addsub">
                    <br>
                    </div>
                    <div class="uploadinput" >
                      <label for="depart">Department :</label>
                      <input type="text" list="depart0" id="depart" autocomplete="off" name="depart" required="required">
                      <datalist id="depart0">
                          <option  >CSE</option>
                          <option  >ECE</option>
                          <option  >MECH</option>
                          <option  >CIVIL</option>
                          <option  >IT</option>    
                          <option  >EEE</option>     
                          <option  >CHE</option>  
                          <option  >BIO</option> 
                          <option  >EIE</option>         
                      </datalist>
                      <br>
                    </div>
                    <div class="uploadinput">
                        <label for="reg">Regulation :</label>
                        <input type="number" id="reg" autocomplete="off" name="Reg" required="required" placeholder="Eg: 2021" min="4">
                        <br>
                    </div>
                      <input type="file" style="margin-left: 12%;" id="file" accept=".xlsx" name="file" required="required">
                      <div class="uploadinput" >
                        <label for="batch">Batch :</label>
                        <input type="text" list="batch0" id="batch" autocomplete="off" name="batch" required="required" disabled="disabled">
                        <datalist id="batch0">
                            <option  id="batch1"></option>
                            <option  id="batch2"></option>
                            <option  id="batch3"></option>
                            <option  id="batch4"></option>
                            <option  id="batch5"></option>         
                        </datalist>
                        <br>
                    </div>
                    <div class="uploadinput">
                      <label for="sem">Semester :</label>
                      <input type="text" id="sem" list="sem0"  name="semester" required="required" disabled="disabled">
                      <datalist id="sem0">
                          <option  id="">Semester 1</option>
                          <option  id="">Semester 2</option>
                          <option  id="">Semester 3</option>
                          <option  id="">Semester 4</option>
                          <option  id="">Semester 5</option>
                          <option  id="">Semester 6</option>
                          <option  id="">Semester 7</option>
                          <option  id="">Semester 8</option>        
                      </datalist>
                      <br>
                  </div>
                  <div class="uploadinput" >
                    <label for="sub">Subject :</label>
                    <input type="text" id="sub" autocomplete="off" name="sub" required="required"disabled="disabled" >
                    <br>
                  </div>
                  <div class="uploadinput">
                    <label for="co">No of CO's :</label>
                    <input type="number" id="co" autocomplete="off" name="co" required="required"disabled="disabled" >
                    <br>
                </div>
                  <button type="submit" style="margin-left: 40%;margin-top: 20px;">Submit</button>
                  </form>
              </div>
        </div>   
        <script>
            var y=new Date().getFullYear();
            for(i=0;i<5;i++)
            document.getElementById("batch"+(i+1)).innerHTML=""+(y-i)+"-"+(y-i+4);
            var status='<%-stats%>'
            status=Number(status);
            if(status==1)
            window.alert("Successfully Updated");
            var t1=0;
            document.getElementById("adsub").addEventListener("click",function()
            {
              if(t1==0)
              {
                
                document.getElementById("sem").removeAttribute("disabled");
                document.getElementById("batch").removeAttribute("disabled");
                document.getElementById("sub").removeAttribute("disabled");
                document.getElementById("co").removeAttribute("disabled");
                document.getElementById("file").setAttribute("disabled","disabled");
                document.getElementById("reg").setAttribute("disabled","disabled");
                t1=1;
                
              }
              else {
                document.getElementById("sem").setAttribute("disabled","disabled");
                document.getElementById("batch").setAttribute("disabled","disabled");
                document.getElementById("sub").setAttribute("disabled","disabled");
                document.getElementById("co").setAttribute("disabled","disabled");
                document.getElementById("file").removeAttribute("disabled");
                document.getElementById("reg").removeAttribute("disabled");
                t1=0;
              }
            });
        </script>   
    </body>
</html>